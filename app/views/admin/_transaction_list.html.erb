<% som_table_for job_stati, :style => "width: 100%" do |st| %>
  <% st.column("") {|js| button_to_function("Details", "audit_details(#{js.job.job_set.id});") } %>
  <% st.column("MRN") {|js| js.exam.patient.mrn } %>
  <% st.column("Accession #") {|js| js.exam.accession_number } %>
  <% st.column("Single Use Patient ID") {|js| js.job.job_set.single_use_patient_id } %>
  <% st.column("Job ID") {|js| js.job.id } %>
  <% st.column("Status Code") {|js| js.status } %>
  <% st.column("Comments") {|js| js.status_message } %>
  <% st.column("Last Transaction") {|js| js.last_transaction_timestamp.to_s(:long) } %>
<% end %>
