<% som_table_for job_transactions, :style => "width: 100%" do |st| %>
  <% st.column("") {|jt| button_to_function("Details", "audit_details(#{jt.id});") } %>
  <% st.column("MRN") {|jt| jt.job.job_set.patient.mrn } %>
  <% st.column("Accession #") {|jt| jt.job.exam.accession_number } %>
  <% st.column("Job ID") {|jt| jt.job.id } %>
  <% st.column("Status Code") {|jt| jt.status_code } %>
  <% st.column("Comments") {|jt| jt.comments } %>
  <% st.column("Document ID") {|jt| jt.job.document_id } %>
  <% st.column("Last Modified") {|jt| jt.modified_date.to_s(:long) } %>
<% end %>
